programa {
	funcao inicio() {
		inteiro num1
		inteiro num2
		inteiro soma
		  escreva("Bem vindo, por favor, informe um numero" + "\n")
		  leia(num1)
		  escreva("por favor, informe outro numero" + "\n")
		  leia(num2)
		  soma = num1 + num2
		  se (soma>10){
		      escreva(soma)
		  } senao escreva("Resultado da soma menor que 10, tente novamente")
		  
	}
}
